  "author": "Hani Lim",
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "clone-deep": "^4.0.0",
    "deep-copy": "^1.4.2",
    "deepcopy": "^1.0.0",
    "http-server": "^0.11.1",
    "immutable": "^3.8.2",
    "lodash.clonedeep": "^4.5.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^3.12.0"
  }
