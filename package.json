{
  "name": "clonedeep-test",
  "version": "1.0.0",
  "description": "barebones directory to reproduct error with clonedeep + --optimize-minimize + source-map in webpack 3",
  "main": "index.js",
  "scripts": {
    "start": "http-server & npm run watch",
    "test1": "http-server & webpack --watch --config webpack.config.js",
