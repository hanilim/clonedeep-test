    "test3": "http-server & webpack --watch --optimize-minimize --config webpack.config2.js",
    "test4": "http-server & webpack --watch -p --config webpack.config3.js",
    "test5": "http-server & webpack --watch -p --config webpack.config4.js",
    "test6": "http-server & webpack --watch -p --config webpack.config5.js",
    "test-deep-copy": "http-server & webpack --watch -p --config webpack.deep-copy.js",
    "watch": "webpack --watch --config webpack.config.js",
    "build": "webpack -p --config webpack.config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
